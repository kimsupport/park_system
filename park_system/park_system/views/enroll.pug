doctype html
html
  head
    link(rel="stylesheet", href="/stylesheets/styleEnroll.css")
  body 
    .whole  
      form.login100-form.validate-form(method="POST")
        span.login100-form-title
          | Manager Enroll
        .wrap-input100.validate-input(data-validate='Valid email is required: ex@abc.xyz')
          input.input100(type='text' name='id' placeholder='ID')
          span.focus-input100
          span.symbol-input100
            i.fa.fa-envelope(aria-hidden='true')
        .wrap-input100.validate-input(data-validate='Password is required')
          input.input100(type='password' name='password' placeholder='PASSWORD')
          span.focus-input100
          span.symbol-input100
            i.fa.fa-lock(aria-hidden='true')
        .container-login100-form-btn
          button.login100-form-btn.enroll(type="submit")
            | Enroll
      -if (try_id) {
      p 이미 있는 아이디 입니다. 다른 아이디를 등록해주세요.
      -}
      
  script(src="https://code.jquery.com/jquery-3.6.0.min.js" integrity="sha256-/xUj+3OJU5yExlq6GSYGSHk7tPXikynS7ogEvDej/m4=" crossorigin="anonymous")
  script(src="/javascripts/scriptEnroll.js")